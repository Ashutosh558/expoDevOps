        {
           steps{
               sh 'echo "This is 1st stage"'
           } 
        }
        stage('stage 2')
        {
            steps{
                sh 'echo "This is 2nd stage"'
            }
        }
        stage('stage 3')
        {
            steps{
                sh 'echo "this is 3rd stage"'
         }
        }
    }
}
